{
  "name": "webpack-curso",
  "version": "1.0.0",
  "description": "Aprendiendo webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:externo": "webpack --config ./externo/webpack.config.js",
    "build:multiple": "webpack --config ./multi-entrypoints/webpack.config.js",
    "build:css": "webpack --config ./css-style-load/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --watch",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js --watch",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js --watch"
  },
  "author": "@ByChanderZap",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.11.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "babel-loader": "8.1.0",
    "css-loader": "4.2.0",
    "html-webpack-plugin": "4.3.0",
    "mini-css-extract-plugin": "0.9.0",
    "style-loader": "1.2.1",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
